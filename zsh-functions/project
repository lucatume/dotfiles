# Connect to BoSez stagin site.
function bosez-staging-ssh(){
		ssh root@165.22.30.192 -t "cd /srv/www/bosez; bash --login"
}

# Open BoSez staging site wp-admin.
function bosez-staging-wp-admin(){
		open http://165.22.30.192/wp-admin
}

# Connect to theaveragedev.com site. 
function theavera-ssh(){
		ssh theavera@theaveragedev.com -p18765 	
}
